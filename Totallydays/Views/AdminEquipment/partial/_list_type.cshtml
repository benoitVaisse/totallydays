@model IEnumerable<Equipment_type>;
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

    <div class="list-type-e">
        @if (Model.Count() > 0)
        {
            @foreach (Equipment_type type in Model)
            {
                <div>
                    <span class="e-type-single" data-id="@type.Equipment_type_id"> @type.Name </span>
                </div>
                
            }
        }
        else
        {
            <span> Aucun type d'equipement </span>
        }

    </div>
